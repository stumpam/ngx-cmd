    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "copy-readme": "copy ./README.md ./dist/libs/ngx-cmd",
    "copy-license": "copy ./LICENSE ./dist/libs/ngx-cmd",
    "copy-changelog": "copy ./Changelog.md ./dist/libs/ngx-cmd",
    "copy-files": "run-s copy-readme copy-license",
    "bump-version:major": "npm version major --force && cd libs/ngx-cmd && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/ngx-cmd && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/ngx-cmd && npm version patch --force",
    "build:lib-pack": "ng build ngx-cmd && npm run copy-files && cd dist/libs/ngx-cmd && npm pack",
    "pre-publish:minor": "run-s bump-version:minor build:lib-pack",
    "pre-publish:major": "run-s bump-version:major build:lib-pack",
    "pre-publish:patch": "run-s bump-version:patch build:lib-pack",
    "publish": "npm publish --access public ./dist/libs/ngx-cmd/stumpam-ngx-cmd-1.1.0.tgz"
  },
  "private": true,
